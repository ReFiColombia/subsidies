{
  "buildCommand": "cd backend && npm install && npm run vercel-build",
  "outputDirectory": "backend/dist",
  "installCommand": "cd backend && npm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/backend/src/index.ts"
    },
    {
      "source": "/health",
      "destination": "/backend/src/index.ts"
    }
  ]
}
